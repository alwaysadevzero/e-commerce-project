    "@taiga-ui/cdk": "^3.39.1",
    "@taiga-ui/core": "^3.39.1",
    "@taiga-ui/icons": "^3.39.1",
    "@taiga-ui/kit": "^3.39.1",
    "@taiga-ui/layout": "^3.39.1",
    "@taiga-ui/styles": "^3.39.1",
    "@tinkoff/ng-dompurify": "3.0.0",
